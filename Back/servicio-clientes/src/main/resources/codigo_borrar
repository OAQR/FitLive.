                        -- Tabla para almacenar la información de los clientes cacheados
                        CREATE TABLE clientes (
                            id BIGSERIAL PRIMARY KEY,
                            ruc VARCHAR(11) NOT NULL UNIQUE,
                            razon_social VARCHAR(255),
                            direccion TEXT,
                            estado VARCHAR(50),
                            departamento VARCHAR(100)
                        );

                        -- Tabla principal para las facturas
                        CREATE TABLE facturas (
                            id BIGSERIAL PRIMARY KEY,
                            serie VARCHAR(4) NOT NULL,
                            correlativo VARCHAR(8) NOT NULL,
                            fecha_emision DATE NOT NULL,
                            ruc_cliente VARCHAR(11) NOT NULL,
                            subtotal NUMERIC(10, 2) NOT NULL,
                            total_igv NUMERIC(10, 2) NOT NULL,
                            importe_total NUMERIC(10, 2) NOT NULL,
                            codigo_hash VARCHAR(64)
                        );

                        -- Tabla para los ítems de cada factura
                        CREATE TABLE factura_items (
                            id BIGSERIAL PRIMARY KEY,
                            producto VARCHAR(255) NOT NULL,
                            cantidad INTEGER NOT NULL,
                            precio_unitario NUMERIC(10, 2) NOT NULL,
                            factura_id BIGINT NOT NULL,
                            CONSTRAINT fk_factura
                                FOREIGN KEY(factura_id)
                                REFERENCES facturas(id)
                        );",
